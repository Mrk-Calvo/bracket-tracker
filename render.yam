services:
  - type: web
    name: bracket-inventory-tracker
    env: python
    plan: free
    branch: main
    buildCommand: |
      pip install -r requirements.txt
    startCommand: |
      python app.py
    envVars:
      - key: SECRET_KEY
        generateValue: true
      - key: PYTHON_VERSION
        value: 3.11.0
